{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-unused-vars": "warn",
        "indent": [
            "warn",
            4,
            {
                "ignoredNodes": ["ConditionalExpression"],
                "SwitchCase": 1
            }
        ]
    }
}
